ilename="/Users/evgeniakveliashvili/Desktop/DSI_projects/sql/02_activities/assignments/assignment2.sql">-- Reference to file &quot;/Users/evgeniakveliashvili/Desktop/DSI_projects/sql/02_activities/assignments/assignment2.sql&quot; (not supported by this version) --</sql><sql name="SQL 6*">SELECT
 distinct p. product_id
, p.product_name
FROM product as p
LEFT join vendor_inventory as v
on p.product_id = v. product_id;


SELECT 
product_id
, quantity as latest_quantity
, market_date
FROM (

SELECT 
product_id
, quantity
, market_date
, row_number() OVER (PARTITION by product_id ORDER by market_date DESC ) as rank
from vendor_inventory)
WHERE rank = 1;



SELECT  DISTINCT product_id
from vendor_inventory
ORDER by 1;

SELECT DISTINCT product_id
from TEMp. product_units
order by 1;

SELECT *
, row_number() OVER (PARTITION by product_id ORDER by market_date DESC ) as rank
from vendor_inventory)
WHERE rank = 1;

SELECT pu.product_id
from  temp. product_units pu
left join ( 
SELECT *
, row_number() OVER (PARTITION by product_id ORDER by market_date DESC ) as rank
from vendor_inventory) as x
on pu.product_id = x.product_id
where rank = 1;

update temp. product_units
SET current_quantity = ( 
SELECT coalesce (x.quantity, 0)  
from (SELECT pu.product_id
, x.quantity
from  temp. product_units pu
left join 
(  SELECT *
,row_number() OVER (PARTITION by product_id ORDER by market_date DESC ) as rank
from vendor_inventory) as x
on pu.product_id = x.product_id
where rank = 1) y
WHERE product_units.product_id = y.product_id)

</sql><sql name="SQL 7*">UPDATE temp.product_units
