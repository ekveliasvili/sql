</sql><sql name="SQL 9*">UPDATE temp.product_units pu
SET current_quantity = (
    SELECT COALESCE(r.quantity, 0)
    FROM (
        SELECT 
            vi.product_id,
            vi.quantity,
            ROW_NUMBER() OVER (PARTITION BY vi.product_id ORDER BY vi.market_date DESC) AS rank
        FROM vendor_inventory vi
    ) r
    WHERE r.product_id = pu.product_id AND r.rank = 1
);
